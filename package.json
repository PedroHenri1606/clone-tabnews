{
  "name": "cuso-dev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run docker:up && next dev",
    "build": "npm run docker:build && next build",
    "start": "npm run docker:up && next start",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker compose -f infra/compose.yaml build",
    "docker:up": "docker compose -f infra/compose.yaml up -d",
    "docker:start": "docker compose -f infra/compose.yaml start",
    "docker:stop": "docker compose -f infra/compose.yaml stop",
    "docker:down": "docker compose -f infra/compose.yaml down"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.4.1"
  }
}
